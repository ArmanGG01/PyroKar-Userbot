_starter () {
    echo "Downloading source code"
    git clone https://github.com/ArmanGG01/PyroKar-Userbot
    cd PyroKar-Userbot/ && rm -rf .git
    python3 ../updater.py ../requirements.txt requirements.txt
    echo "PyoKar-Userbot v0.2.0, Copyright Â© 2022 (Arman) <https://github.com/ArmanGG01>"
    python3 -m PyroKar
}

_starter
